
<div class="jumbotron">
  <h1 class="display-4">  <%= @client.fname%>  <%= @client.lname %> </h1>
  <p class="lead"><%= @client.mealType %></p>
  <hr class="my-4">
  <p><%= @client.phone %> | <%= @client.email %> | <%= @client.address %> </p>
  <p> <%= @client.notes%></p>


  <p class="lead">
    <%= link_to 'Edit', edit_client_path(@client),class:"btn btn-primary" %>
    <%= link_to 'Back', clients_path,class:"btn btn-primary" %>
    <td><%= link_to 'Destroy', @client, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-outline-danger" %></td>
    
  </p>
</div>

 